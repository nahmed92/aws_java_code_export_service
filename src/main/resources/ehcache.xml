<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="true" monitoring="autodetect" dynamicConfig="true">
    <defaultCache
           maxElementsInMemory="300" eternal="false"
           timeToLiveSeconds="30"
           overflowToDisk="false"
           memoryStoreEvictionPolicy="LFU" transactionalMode="off">
   </defaultCache>
    <cache name="attribute-service-cache"
           maxElementsInMemory="1000" eternal="false"
           timeToLiveSeconds="30"
           overflowToDisk="false"
           memoryStoreEvictionPolicy="LFU" transactionalMode="off">
    </cache>
    <cache name="locale-service-cache"
           maxElementsInMemory="300" eternal="false"
           timeToLiveSeconds="30"
           overflowToDisk="false"
           memoryStoreEvictionPolicy="LFU" transactionalMode="off">
    </cache>
    <cache name="industry-service-cache"
           maxElementsInMemory="100" eternal="false"
           timeToLiveSeconds="300"
           overflowToDisk="false"
           memoryStoreEvictionPolicy="LFU" transactionalMode="off">
    </cache>
    <cache name="category-service-cache"
           maxElementsInMemory="300" eternal="false"
           timeToLiveSeconds="30"
           overflowToDisk="false"
           memoryStoreEvictionPolicy="LFU" transactionalMode="off">
    </cache>
    <cache name="media-attribute-service-cache"
           maxElementsInMemory="300" eternal="false"
           timeToLiveSeconds="100"
           overflowToDisk="false"
           memoryStoreEvictionPolicy="LFU" transactionalMode="off">
    </cache>
    <cache name="translation-service-cache"
           maxElementsInMemory="1000" eternal="false"
           timeToLiveSeconds="100"
           overflowToDisk="false"
           memoryStoreEvictionPolicy="LFU" transactionalMode="off">
    </cache>
</ehcache>
